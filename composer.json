{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.1",
		"nette/di": "^3.1",
		"nette/forms": "^3.1",
		"nette/http": "^3.2",
		"nette/mail": "^3.0",
		"nette/robot-loader": "^3.0",
		"nette/security": "^3.1",
		"nette/utils": "^3.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.9",
		"contributte/console": "^0.9.3",
		"nextras/migrations": "^3.2",
		"nextras/orm": "^4.0",
		"nextras/forms-rendering": "2.0.x-dev",
		"nextras/mail-panel": "^2.6",
		"contributte/forms-multiplier": "^4.0",
		"ublaboo/datagrid": "^6.9",
		"ijvo/jquery-fileupload": "^3.0",
		"ezyang/htmlpurifier": "^4.17"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"symfony/thanks": "^1",
		"contributte/nextras-orm-generator": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": false
		},
		"platform": {
			"php": "8.1.17"
		}
	}
